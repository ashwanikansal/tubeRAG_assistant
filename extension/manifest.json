{
  "manifest_version": 3,
  "name": "TubeRAG Assistant",
  "version": "1.0",
  "description":"AI-powered YouTube assistant using RAG. Ask questions about videos and get answers from English transcripts instantly.",
  "icons":{
    "16":"icons/logo_icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab","storage"],
  "host_permissions": [
    "https://www.youtube.com/*",
    "http://localhost:8000/*"
  ]
}
